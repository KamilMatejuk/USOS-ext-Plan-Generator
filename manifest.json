{
  "manifest_version": 3,
  "version": "0.0.0.1",
  "name": "USOS Plan Generator",
  "description": "Plan you class schedule in USOS",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "js": [
          "styles.js",
          "read_plan.js",
          "add_options.js",
          "edit_timetable.js",
          "storage.js",
          "main.js"],
      "matches": ["https://*.edu.pl/*"]
    }
  ]
}
